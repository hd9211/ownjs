                        <source src="https://flv2.bn.netease.com/5e7e1d73ad0d13589fba938643ccaae1effaf5baf5e6a18eca57c30d997f8df4bb2b6094cc2caaa2b1143f6ebc25cda2982db4a238de84b30dd70306a3a2af9947fce47f8a50d1a40502ffeef977801b803cd662b28f634df424d3a601883d4bd0ccf88375a202ca07a03d3a56e0aa7dd05926a9da868f5f.mp4" type="video/mp4">
                        <source src="https://flv2.bn.netease.com/5e7e1d73ad0d13589fba938643ccaae1effaf5baf5e6a18eca57c30d997f8df4bb2b6094cc2caaa2b1143f6ebc25cda2982db4a238de84b30dd70306a3a2af9947fce47f8a50d1a40502ffeef977801b803cd662b28f634df424d3a601883d4bd0ccf88375a202ca07a03d3a56e0aa7dd05926a9da868f5f.mp4" type="video/m3u8">
