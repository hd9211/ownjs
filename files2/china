            <pubDate>Thu, 26 Nov 2020 10:42:00 GMT</pubDate>
            <guid isPermaLink="false">https://www.zaobao.com/realtime/china/story20201126-1104147</guid>
            <link>https://www.zaobao.com/realtime/china/story20201126-1104147</link>
            
            
            
            
            
                
            
            
